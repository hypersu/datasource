{
  "name": "sqlserver",
  "class": "com.hs.datasource.plugin.sqlserver.SqlServerDataSource",
  "description": "sqlserver数据源2005版本及以上",
  "attributes": {
    "user": {
      "value": null,
      "type": "string",
      "required": true,
      "visible": true,
      "description": "用户名称"
    },
    "password": {
      "value": null,
      "type": "string",
      "required": true,
      "visible": true,
      "description": "用户密码"
    },
    "host": {
      "value": null,
      "type": "string",
      "required": true,
      "visible": true,
      "description": "主机地址"
    },
    "port": {
      "value": 1521,
      "type": "integer",
      "required": true,
      "visible": true,
      "description": "主机端口"
    },
    "database": {
      "value": null,
      "type": "string",
      "required": true,
      "visible": true,
      "description": "实例名/服务名"
    },
    "properties": {
      "value": null,
      "type": "map",
      "required": false,
      "visible": true,
      "description": "属性列表"
    }
  },
  "meta": {
    "type": [
      "catalog",
      "schema",
      "table",
      "column",
      "primary"
    ],
    "catalog": {
      "hierarchy": "catalog.%{catalog}",
      "parent_type": null,
      "fields": {
        "catalog": {
          "type": "string",
          "description": "库名"
        }
      }
    },
    "schema": {
      "hierarchy": "catalog.%{catalog}.schema.%{schema}",
      "parent_type": "catalog",
      "fields": {
        "catalog": {
          "type": "string",
          "description": "库名"
        },
        "schema": {
          "type": "string",
          "description": "架构名"
        }
      }
    },
    "table": {
      "hierarchy": "catalog.%{catalog}.schema.%{schema}.table.%{table_name}",
      "parent_type": "schema",
      "fields": {
        "catalog": {
          "type": "string",
          "description": "库名"
        },
        "schema": {
          "type": "string",
          "description": "架构名"
        },
        "table_name": {
          "type": "string",
          "description": "表名"
        },
        "comment": {
          "type": "string",
          "description": "表注释"
        }
      }
    },
    "column": {
      "hierarchy": "catalog.%{catalog}.schema.%{schema}.table.%{table_name}.column.%{column_name}",
      "parent_type": "table",
      "fields": {
        "catalog": {
          "type": "string",
          "description": "库名"
        },
        "schema": {
          "type": "string",
          "description": "架构名"
        },
        "table_name": {
          "type": "string",
          "description": "表名"
        },
        "column_name": {
          "type": "string",
          "description": "字段名"
        },
        "data_type": {
          "type": "integer",
          "description": "数据类型"
        },
        "type_name": {
          "type": "string",
          "description": "类型名称"
        },
        "length": {
          "type": "integer",
          "description": "字段长度"
        },
        "precision": {
          "type": "integer",
          "description": "字段精度"
        },
        "scale": {
          "type": "integer",
          "description": "字段标度"
        },
        "primary": {
          "type": "boolean",
          "description": "是否主键"
        },
        "nullable": {
          "type": "boolean",
          "description": "是否可空"
        },
        "comment": {
          "type": "string",
          "description": "字段注释"
        },
        "sort": {
          "type": "integer",
          "description": "字段排序"
        }
      }
    },
    "primary": {
      "hierarchy": "catalog.%{catalog}.schema.%{schema}.table.%{table_name}.primary.%{column_name}",
      "parent_type": "table",
      "fields": {
        "catalog": {
          "type": "string",
          "description": "库名"
        },
        "schema": {
          "type": "string",
          "description": "架构名"
        },
        "table_name": {
          "type": "string",
          "description": "表名"
        },
        "column_name": {
          "type": "string",
          "description": "字段名"
        },
        "key_seq": {
          "type": "int",
          "description": "字段序号"
        },
        "pk_name": {
          "type": "string",
          "description": "主键名"
        }
      }
    }
  }
}